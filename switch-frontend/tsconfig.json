{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/typings.d.ts",
    "src/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/.umi/**/*.ts",
    "src/.umi/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "lib",
    "es",
    "dist",
    "build"
  ]
}
